{
  "name": "guidesmith-back",
  "version": "0.0.1",
  "description": "guidesmith",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type":"commonjs",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run rebuild",
    "docker:build": "docker build -t guidesmith-back .",
    "docker:run": "docker run -p 3000:3000 -d guidesmith-back",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Tihomir Stoychev Stoychev <tihomir.stoychev@mhp-net.de>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^4.1.2",
    "@loopback/core": "^3.1.2",
    "@loopback/repository": "^4.1.2",
    "@loopback/rest": "^11.1.2",
    "@loopback/rest-explorer": "^4.1.2",
    "@loopback/service-proxy": "^4.1.2",
    "loopback-connector-mongodb": "^5.6.0",
    "mongodb": "^4.5.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@loopback/testlab": "^4.1.2",
    "@types/node": "^12.20.47",
    "eslint": "^8.10.0",
    "mocha": "^9.2.2",
    "source-map-support": "^0.5.21",
    "typescript": "~4.6.3"
  }
}
